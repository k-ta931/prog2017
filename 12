def func():
 i = 3
 return i

func()

def func(v):
 i = v + 3
 return i

func(5) 

def total(n):
 s=0
 for x in range(1,n+1):
    s+=x
 return s
 
import turtle
turtle = turtle.Turtle()
def center_circle():
 turtle.penup()
 turtle.forward(200)
 turtle.left(90)
 turtle.pendown()
 turtle.circle(200)
 turtle.left(90)
 turtle.penup()
 turtle.forward(200)
 turtle.pendown()

center_circle()


